{
  "manifest_version": 3,
  "name": "Dumpster Duck",
  "version": "0.1.0",
  "description": "search where to put your waste quickly with a snap of a pic",
  "icons": {
    "16": "icons/Dumpster Duck (Transparent, 16x16).png",
    "48": "icons/Dumpster Duck (Transparent, 48x48).png",
    "128": "icons/Dumpster Duck (Transparent, 128x128).png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Dumpster Duck",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}